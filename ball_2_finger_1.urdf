<?xml version="1.0"?>
<robot name="spherical_actuators">

  <!-- Model 0 -->
  <link name="intermediate_x_0"/>
  <link name="intermediate_y_0"/>
  <link name="sphere_0">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="2e-5" ixy="0" ixz="0" iyy="2e-5" iyz="0" izz="2e-5"/>
